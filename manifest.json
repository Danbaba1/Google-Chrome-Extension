{
  "manifest_version": 3,
  "name": "Focus",
  "version": "1.0",
  "description": "Focus chrome extension",
  "action": {
    "default_icon": "delete.png",
    "default_popup": "popup.html" 
  },
  "background": {
    "service_worker": "main.js" 
  },
  "permissions": ["https://www.youtube.com/*"], 
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"] 
    }
  ]
}
